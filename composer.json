{
  "name": "cheetah/cheetah",
  "version": "1.0.0.1",
  "description": "一个协程化的PHP高性能框架",
  "authors": [
    {
      "name": "kelezyb",
      "email": "eicesoft@qq.com",
      "role": "developer"
    }
  ],
  "license": "MIT",
  "keywords": [
    "php",
    "swoole",
    "cheetah",
    "microservice",
    "coroutine"
  ],
  "homepage": "https://cheetah.io",
  "support": {
    "docs": "https://wiki.cheetah.wiki",
    "issues": "https://github.com/eicesoft/cheetah/issues",
    "source": "https://github.com/eicesoft/cheetah"
  },
  "require": {
    "ext-pdo": "*",
    "ext-posix": "*",
    "psr/http-server-handler": "^1.0",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/log": "^1.1",
    "psr/event-dispatcher": "^1.0",
    "php-di/php-di": "^6.2",
    "ocramius/proxy-manager": "^2.8"
  },
  "require-dev": {
    "doctrine/annotations": "~1.2",
    "swoole/ide-helper": "~4.4.7"
  },
  "replace": {
    "cheetah/core": "self.version",
    "cheetah/server": "self.version",
    "cheetah/file": "self.version",
    "cheetah/config": "self.version",
    "cheetah/util": "self.version",
    "cheetah/di": "self.version"
  },
  "autoload": {
    "files": [
      "src/cheetah-util/src/function.php"
    ],
    "psr-4": {
      "Cheetah\\": "src/cheetah-core/src/",
      "Cheetah\\Server\\": "src/cheetah-server/src/",
      "Cheetah\\File\\": "src/cheetah-file/src/",
      "Cheetah\\Config\\": "src/cheetah-config/src/",
      "Cheetah\\Util\\": "src/cheetah-util/src/",
      "Cheetah\\Di\\": "src/cheetah-di/src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
